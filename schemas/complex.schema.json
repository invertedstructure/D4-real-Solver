{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ChainComplexGF2",
  "type": "object",
  "required": [
    "boundaries"
  ],
  "properties": {
    "boundaries": {
      "type": "object",
      "description": "d_k: C_k -> C_{k-1}, entries in {0,1}. Keys are degrees as strings.",
      "patternProperties": {
        "^[0-9]+$": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "integer",
              "enum": [
                0,
                1
              ]
            }
          },
          "minItems": 0
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}